\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang3081{\*\listtag0}cisternae, ribosome, endoplasmic reticulum, Golgi apparatus, Golgi}{\s28\f0\fs24\lang3081{\*\listtag0}\par}
